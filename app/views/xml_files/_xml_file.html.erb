<tr>
	<td>
		<%= xml_file.file_name%>
	</td>
	<td>
		<%= xml_file.file_path%>
	</td>
	<td>
		<%= xml_file.status%>
	</td>
	<td>
		<%= link_to xml_file, target: "_blank", class: "btn btn-info" do %>
			<i class="fas fa-eye"></i>
		<%end%>
		<%= link_to edit_xml_file_path(xml_file), target: "_blank", class: "btn btn-warning" do %>
			<i class="fas fa-edit"></i>
		<%end%>

		<%= link_to xml_file, method: :delete, data: { confirm: "Are you sure you want to delete this file?"}, class: "btn btn-danger" do %>
			<i class="fas fa-trash"></i>
		<%end%>
	</td>
</tr>
